# Given data
age <- c(13,15,16,16,19,20,20,21,22,22,25,25,25,25,30,33,33,35,35,35,35,36,40,45,46,52,70)

# Mean and Median
mean(age)
median(age)

# Mode
tbl <- table(age)
names(tbl[tbl == max(tbl)])

# Midrange
(min(age) + max(age)) / 2

# Quartiles
quantile(age, c(0.25, 0.75))

OUTPUT 
[1] 29.96296
[1] 25
[1] "25" "35"
[1] 41.5
 25%  75% 
20.5 35.0 
